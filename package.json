{
  "name": "modern-nextjs-app",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack --hostname 0.0.0.0",
    "dev:debug": "NODE_OPTIONS='--inspect=0.0.0.0:9229' next dev --turbopack --hostname 0.0.0.0",
    "build": "next build",
    "start": "next start --hostname 0.0.0.0",
    "start:prod": "NODE_ENV=production next start --hostname 0.0.0.0",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "typecheck": "tsc --noEmit",
    "analyze": "ANALYZE=true next build",
    "clean": "rm -rf .next node_modules",
    "clean:cache": "rm -rf .next node_modules/.cache",
    "prebuild": "npm run typecheck && npm run lint",
    "logs": "tail -f .next/trace",
    "logs:dev": "DEBUG=* npm run dev",
    "test": "echo \"Add your test command here\"",
    "format": "prettier --write .",
    "format:check": "prettier --check ."
  },
  "dependencies": {
    "@prisma/client": "^6.15.0",
    "@vercel/analytics": "^1.5.0",
    "@vercel/speed-insights": "^1.2.0",
    "chart.js": "^4.5.0",
    "clsx": "^2.1.1",
    "framer-motion": "^12.23.12",
    "lucide-react": "^0.542.0",
    "next": "^15.5.2",
    "prisma": "^6.15.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "tailwind-merge": "^3.3.1",
    "zod": "^4.1.5"
  },
  "devDependencies": {
    "@sparticuz/chromium": "^138.0.2",
    "@tailwindcss/postcss": "^4.0.0",
    "@types/node": "^22.0.0",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.34.0",
    "eslint-config-next": "^15.5.2",
    "puppeteer-core": "^24.19.0",
    "tailwindcss": "^4.0.0",
    "typescript": "^5.7.2"
  },
  "packageManager": "npm@10.8.3"
}
